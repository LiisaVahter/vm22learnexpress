{% extends "partials/layout.njk" %}
{% block title %}Home{%endblock%}
{% block content%}
<a href = "/movies/add">Add </a>

<style>
    table, th, tb {
        border: 1px solid black;
    }
</style>

<table>
    <thead>
        <th>Name</th>
        <th>Year</th>
    </thead>
    <tbody>
        {% for movie in movies %}
           <tr>
             <td>{{movie.name}}</td>
             <td>{{movie.year}}</td>
           </tr>
         {% endfor %}
      

      
    </tbody>
</table>


    
{% endblock %}
